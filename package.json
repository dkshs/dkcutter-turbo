{
  "name": "dkcutter-turbo",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.5.2",
  "description": "DKCutter Turbo is a framework for quickly starting projects with Turborepo.",
  "author": {
    "name": "Nicolas Contiero",
    "url": "https://github.com/dkshs"
  },
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "generate": "dkcutter .",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "check": "pnpm lint && pnpm typecheck",
    "commitlint": "commitlint --edit",
    "pre-commit": "lint-staged -c ./package.json",
    "prepare": "husky"
  },
  "dependencies": {
    "dkcutter": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@dkshs/eslint-config": "^4.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.5",
    "colorette": "^2.0.20",
    "eslint": "^9.21.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "type-fest": "^4.35.0",
    "typescript": "^5.7.3",
    "zod": "^3.24.2"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*": "pnpm eslint --fix"
  }
}
